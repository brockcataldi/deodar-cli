{
	"name": "deodar-cli",
	"version": "2.0.0",
	"description": "A powerful CLI tool for building and managing WordPress blocks with Advanced Custom Fields (ACF)",
	"main": "./dist/index.js",
	"bin": {
		"deodar": "./dist/index.js"
	},
	"homepage": "https://github.com/brockcataldi/deodar-cli#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brockcataldi/deodar-cli.git"
	},
	"bugs": {
		"url": "https://github.com/brockcataldi/deodar-cli/issues"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"dev": "ts-node src/index.ts",
		"lint": "npx eslint src/",
		"format": "npx prettier . --write",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"keywords": [
		"wordpress",
		"acf",
		"advanced-custom-fields",
		"gutenberg",
		"blocks",
		"cli",
		"build-tool",
		"scss",
		"javascript",
		"esbuild"
	],
	"author": "Brock Cataldi",
	"license": "GPL-2.0-or-later",
	"engines": {
		"node": ">=18.0.0"
	},
	"files": [
		"dist/",
		"templates/",
		"README.md",
		"LICENSE"
	],
	"type": "module",
	"dependencies": {
		"@inquirer/prompts": "^7.8.6",
		"@types/archiver": "^6.0.3",
		"@types/micromatch": "^4.0.9",
		"archiver": "^7.0.1",
		"chalk": "^5.6.2",
		"change-case": "^5.4.4",
		"chokidar": "^4.0.3",
		"commander": "^14.0.1",
		"esbuild": "^0.25.10",
		"esbuild-sass-plugin": "^3.3.1",
		"micromatch": "^4.0.8",
		"mustache": "^4.2.0",
		"sass": "^1.93.2",
		"slugify": "^1.6.6"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@types/mustache": "^4.2.6",
		"@types/node": "^24.6.1",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.4.0",
		"prettier": "3.6.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.45.0",
		"vitest": "^3.2.4"
	}
}
